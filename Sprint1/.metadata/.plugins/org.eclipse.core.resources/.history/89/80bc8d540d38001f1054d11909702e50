package application;

public class While {

	public static void main(String[] args) {
		int contador = 1;

        while (contador <= 5) {
            System.out.println("Contador: " + contador);
            contador++;
        }
        
        while(true) {
        	if(contador == 5) {
        		break;
        	}
        	System.out.println("Contador: " + contador);
            contador++;
        }

	}

}
